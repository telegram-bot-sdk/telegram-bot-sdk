{
  "name": "telegram-bot-sdk/telegram-bot-sdk",
  "type": "library",
  "description": "The Telegram Bot API PHP SDK",
  "keywords": [
    "telegram",
    "chatbot",
    "telegram php",
    "telegram bot",
    "telegram chatbot",
    "telegram bot api",
    "telegram bot sdk",
    "telegram-bot-sdk",
    "telegram bot php sdk",
    "telegram bot api php sdk",
    "php telegram bot"
  ],
  "homepage": "https://github.com/telegram-bot-sdk/telegram-bot-sdk",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Syed Irfaq R.",
      "email": "syed+gh@lukonet.com",
      "homepage": "https://github.com/irazasyed"
    },
    {
      "name": "Telegram Bot SDK Community",
      "homepage": "https://github.com/telegram-bot-sdk/telegram-bot-sdk/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3 || 7.0",
    "guzzlehttp/psr7": "^1.3",
    "illuminate/container": "^6.0 || ^7.0",
    "illuminate/events": "^6.0 || ^7.0",
    "illuminate/support": "^6.0 || ^7.0",
    "telegram-bot-sdk/addon-manager": "^1.0@dev"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.3",
    "phpunit/phpunit": ">=8.5"
  },
  "suggest": {
    "irazasyed/larasupport": "Allows you to use any Laravel Package in Lumen by adding support!"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telegram\\Bot\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
